{ "rules":
    {   "comma-spacing"        : 0
    ,   "consistent-return"    : 0
    ,   "curly"                : 0
    ,   "eol-last"             : 0
    ,   "eqeqeq"               : 0
    ,   "key-spacing"          : 0
    ,   "new-cap"              : 0
    ,   "no-alert"             : 0
    ,   "no-comma-dangle"      : 0
    ,   "no-console"           : 0
    ,   "no-extend-native"     : 0
    ,   "no-extra-semi"        : 0
    ,   "no-multi-spaces"      : 0
    ,   "no-multi-str"         : 0
    ,   "no-new-func"          : 0
    ,   "no-return-assign"     : 0
    ,   "no-shadow"            : 0
    ,   "no-space-before-semi" : 0
    ,   "no-spaced-func"       : 0
    ,   "no-sparse-arrays"     : 0
    ,   "no-unused-expressions": 0

    ,   "no-use-before-define" :
            [2, "nofunc"]

    ,   "space-infix-ops"      : 0
    ,   "space-unary-ops"      : 0
    ,   "strict"               : 0
    ,   "quotes"               : 0
    ,   "yoda"                 : 0
    },
 
  "env":
    {   "browser" : true
    ,   "node"    : false
    }
}